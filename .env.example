# Leave empty to use the default LogoIcon
NEXT_PUBLIC_APP_LOGO_URL=

# App Name - Your application name
NEXT_PUBLIC_APP_NAME=Onboard Hero

# App Tagline - Your application tagline/subtitle
NEXT_PUBLIC_APP_TAGLINE=AI-Powered Automation

# Development Settings
NEXT_PUBLIC_DEV_AUTO_LOGIN=true

# Database Configuration
DATABASE_URL=

# Authentication
NEXTAUTH_SECRET=
