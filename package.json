{
  "name": "onboardkit",
  "version": "0.1.0",
  "description": "OnboardKit - SaaS platform for creating professional client onboarding experiences",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "type-check:strict": "tsc --noEmit --project tsconfig.strict.json",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "db:generate-types": "npx supabase gen types typescript --project-id $NEXT_PUBLIC_SUPABASE_PROJECT_REF > lib/supabase/database.types.ts",
    "db:setup": "chmod +x scripts/setup-database-simple.sh && ./scripts/setup-database-simple.sh",
    "db:setup-full": "chmod +x scripts/setup-database.sh && ./scripts/setup-database.sh",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset",
    "deploy:vercel": "chmod +x scripts/deploy-production.sh && ./scripts/deploy-production.sh vercel production",
    "deploy:netlify": "chmod +x scripts/deploy-production.sh && ./scripts/deploy-production.sh netlify production",
    "deploy:aws": "chmod +x scripts/deploy-production.sh && ./scripts/deploy-production.sh aws production",
    "deploy:staging": "chmod +x scripts/deploy-production.sh && ./scripts/deploy-production.sh vercel staging",
    "health-check": "curl -f http://localhost:3000/api/health || exit 1",
    "validate-features": "npm run build && npm run health-check",
    "validate-production": "chmod +x scripts/validate-production-readiness.sh && ./scripts/validate-production-readiness.sh",
    "production-ready": "npm run type-check && npm run test:ci && npm run build && npm run validate-production"
  },
  "dependencies": {
    "@apollo/server": "^5.0.0",
    "@aws-sdk/client-s3": "^3.864.0",
    "@aws-sdk/s3-request-presigner": "^3.864.0",
    "@hookform/resolvers": "^3.3.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.38.0",
    "@tailwindcss/typography": "^0.5.10",
    "@tiptap/pm": "^2.1.13",
    "@tiptap/react": "^2.1.13",
    "@tiptap/starter-kit": "^2.1.13",
    "@upstash/ratelimit": "^0.4.4",
    "@upstash/redis": "^1.25.1",
    "apollo-server-micro": "^3.13.0",
    "autoprefixer": "^10.4.16",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "boldsign": "^1.0.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "dotenv": "^17.2.1",
    "graphql": "^16.11.0",
    "graphql-scalars": "^1.24.2",
    "graphql-tag": "^2.12.6",
    "lucide-react": "^0.294.0",
    "next": "^14.2.31",
    "next-pwa": "^5.6.0",
    "nylas": "^7.11.0",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-datepicker": "^4.24.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.48.2",
    "recharts": "^3.1.2",
    "resend": "^6.0.1",
    "sharp": "^0.33.0",
    "stripe": "^14.7.0",
    "swr": "^2.2.4",
    "tailwind-merge": "^2.1.0",
    "tailwindcss": "^3.3.6",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.3.3",
    "workbox-webpack-plugin": "^7.3.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.9.2",
    "@types/react": "^18.2.37",
    "@types/react-datepicker": "^4.19.4",
    "@types/react-dom": "^18.2.15",
    "eslint": "^8.54.0",
    "eslint-config-next": "14.0.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "playwright": "^1.40.0",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7"
  },
  "keywords": [
    "saas",
    "onboarding",
    "nextjs",
    "typescript",
    "supabase",
    "tailwindcss",
    "stripe"
  ],
  "author": "OnboardKit",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "npm@10.9.2"
}
